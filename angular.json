        }
      }
    },
    "client-data": {
      "projectType": "library",
      "root": "libs/client-data",
      "sourceRoot": "libs/client-data/src",
      "prefix": "cleric-eshop",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
