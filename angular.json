          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/client-data"],
          "options": {
            "jestConfig": "libs/client-data/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "client-state": {
      "projectType": "library",
      "root": "libs/client-state",
      "sourceRoot": "libs/client-state/src",
      "prefix": "cleric-eshop",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
